---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="{{ site.lang }}">
  <id>tag:achretien.fr,2014-03-15:journal</id>
  <title>{{ site.title }}</title>
  <link href="{{ "/feed.atom" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/atom+xml" />
  <link href="{{ site.baseurl | preprend: site.url }}" rel="alternate" type="text/html" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <author>
    <name>{{ site.author }}</name>
    <uri>{{ site.url }}</uri>
  </author>
  {% for post in site.posts %}
  <entry>
    <id>tag:achretien.fr,{{ post.date | date: "%Y-%m-%d" }}:{{ post.slug }}</id>
    <link rel="alternate" type="text/html" href="{{ post.url | prepend: site.baseurl | prepend: site.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.update || post.date | date_to_xmlschema }}</updated>
    <content type="xhtml">
      <div xmlns="http://www.w3.org/1999/xhtml">
        {{ post.content }}
      </div>
    </content>
  </entry>
  {% endfor %}
</feed>