---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="fr">
  <id>tag:{{ site.url }},2014-03-15:blog</id>
  <title>Blog âˆ’ {{ site.name }}</title>
  <link href="http://{{ site.url }}/feed.atom" rel="self" type="application/atom+xml" />
  <link href="http://{{ site.url }}/" rel="alternate" type="text/html" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <author>
    <name>{{ site.author }}</name>
    <uri>http://{{ site.url }}/</uri>
  </author>
  {% for post in site.posts %}
  <entry xml:lang="en">
    <id>tag:{{ site.url }},{{ post.date }}:{{ post.slug }}</id>
    <link rel="alternate" type="text/html" href="http://{{ site.url }}{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">
      <div xmlns="http://www.w3.org/1999/xhtml">
        {{ post.content }}
      </div>
    </content>
  </entry>
  {% endfor %}
</feed>